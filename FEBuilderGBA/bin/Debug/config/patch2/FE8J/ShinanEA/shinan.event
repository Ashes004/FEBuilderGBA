PUSH

ORG	FEBUILDER_EXTRA_ORG	//FEBuilderGBAで確保したメニュー領域に追加
POIN	shinan_string
SHORT	$061A $0646		//右がヘルプ指定
SHORT	$6304 0
POIN	0x80501BC + 1       //show always
POIN	GreenMenu0x26 + 1   //緑色表示
POIN	Shinnan + 1         //指南を実行
WORD	0 0 0

POP

Shinnan:
#incbin "Shinan.dmp"
POIN shinan_table

GreenMenu0x26:
#incbin "GreenMenu0x26.dmp"

ALIGN 4
#include "shinan_table.event"

ALIGN 4
shinan_string:
#incbin "menuname.bin"

ALIGN 4
